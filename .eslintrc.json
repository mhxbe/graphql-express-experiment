{
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "modules": true
    },
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "jest": true,
    "node": true
  },
  "extends": [
    "plugin:prettier/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/react",
    "eslint:recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "import/newline-after-import": "error",
    "import/order": "error"
  }
}
